{
    "defaultSeverity": "error",
    "linterOptions": {
      "exclude": [
        "config/**/*.ts",
        "node_modules/**/*.ts",
        "src/serviceWorker.ts"
      ]
    },
    "extends": [
      "tslint:recommended",
      "tslint:latest",
      "tslint-react",
      "tslint-consistent-codestyle",
      "tslint-import-group-ordering",
      "tslint-config-prettier"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-operator",
      "check-typecast"
    ],
    "jsRules": {},
    "rules": {
      "radix": false,
      "prefer-for-of": false,
      "no-shadowed-variable": false,
      "prefer-template": true,
      "no-console": [
        true,
        "log",
        "error"
      ],
      "no-sparse-arrays": false,
      "no-object-literal-type-assertion": false,
      "prefer-conditional-expression": false,
      "no-duplicate-variable": false,
      "ban-comma-operator": false,
      "jsdoc-format": [
        true,
        "check-multiline-start"
      ],
      "no-implicit-dependencies": false,
      "array-type": false,
      "arrow-return-shorthand": true,
      "curly": [
        true,
        "ignore-same-line"
      ],
      "eofline": false,
      "member-access": true,
      "no-angle-bracket-type-assertion": false,
      "no-bitwise": false,
      "no-internal-module": false,
      "no-this-assignment": [
        true,
        {
          "allowed-names": [
            "^self$"
          ],
          "allow-destructuring": true
        }
      ],
      "no-namespace": [
        true,
        "allow-declarations"
      ],
      "jsx-key": true,
      "jsx-no-bind": true,
      "only-arrow-functions": [
        true,
        "allow-declarations",
        "allow-named-functions"
      ],
      "no-any": false,
      "no-empty": [
        true,
        "allow-empty-catch"
      ],
      "object-literal-sort-keys": false,
      "interface-over-type-literal": false,
      "max-classes-per-file": false,
      "jsx-no-multiline-js": false,
      "jsx-self-close": true,
      "member-ordering": false,
      "ordered-imports": false,
      "jsx-boolean-value": [
        true,
        "never"
      ],
      "jsx-no-lambda": false,
      "jsx-wrap-multiline": false,
      "max-file-line-count": [
        true,
        1200
      ],
      "switch-default": true,
      "jsx-equals-spacing": [
        "always"
      ],
      "no-default-export": false,
      "no-var-keyword": true,
      "no-submodule-imports": false,
      "naming-convention": [
        true,
        {
          "type": "default",
          "format": [
            "camelCase",
            "PascalCase",
            "snake_case"
          ],
          "leadingUnderscore": "forbid",
          "trailingUnderscore": "forbid"
        },
        {
          "type": "variable",
          "modifiers": [
            "global",
            "const"
          ],
          "format": [
            "camelCase",
            "PascalCase",
            "UPPER_CASE",
            "snake_case"
          ]
        },
        {
          "type": "functionVariable",
          "modifiers": [
            "export",
            "const"
          ],
          "format": [
            "PascalCase",
            "camelCase"
          ]
        },
        {
          "type": "parameter",
          "modifiers": "unused",
          "leadingUnderscore": "allow"
        },
        {
          "type": "member",
          "modifiers": "private",
          "leadingUnderscore": "allow"
        },
        {
          "type": "member",
          "modifiers": "protected",
          "leadingUnderscore": "forbid"
        },
        {
          "type": "property",
          "modifiers": [
            "public",
            "static",
            "const"
          ],
          "format": "UPPER_CASE"
        },
        {
          "type": "type",
          "format": "PascalCase"
        },
        {
          "type": "class",
          "modifiers": "abstract",
          "prefix": "Abstract"
        },
        {
          "type": "class",
          "format": "PascalCase"
        },
        {
          "type": "interface",
          "prefix": "I",
          "require": true
        }
      ],
      "import-group-ordering": {
        "severity": "error",
        "options": {
          "imports-groups": [
            {
              "name": "dependencies"
            },
            {
              "name": "common"
            },
            {
              "name": "product"
            },
            {
              "name": "other"
            }
          ],
          "matching-rules": [
            {
              "type": "project",
              "matches": "^(common)",
              "imports-group": "common"
            },
            {
              "type": "project",
              "matches": "^(product)",
              "imports-group": "product"
            },
            {
              "type": "dependencies",
              "imports-group": "dependencies",
              "disable-native-nodejs-modules": true,
              "from-package.json": true
            },
            {
              "type": "project",
              "matches": ".*",
              "imports-group": "other"
            }
          ]
        }
      }
    },
    "rulesDirectory": []
  }
  